{
  "name": "KeepGamersAlive",
  "author": "cabul",
  "version": "0.0.3",
  "main": "./src/main.js",
  "browserify-shim": {
    "tween": "global:TWEEN",
    "pixi": "global:PIXI",
    "gui": "global:dat.GUI"
  },
  "browserify": {
    "transform": [ "browserify-shim" ],
    "paths": ["./node_modules","./src"]
  },
  "watchify": {
    "transform": [ "browserify-shim" ]
  },
  "scripts": {
    "build": "browserify . -o ./bin/bundle.js -d",
    "watch": "watchify . -o ./bin/bundle.js -v -d",
    "server": "http-server"
  },
  "dependencies": {
  },
  "devDependencies": {
    "browserify": "*",
    "watchify": "*",
    "browserify-shim": "*",
    "http-server": "*"
  }
}
